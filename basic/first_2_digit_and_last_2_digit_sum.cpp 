#include<iostream>
using namespace std;

int main(){
    int sum ,anotherSum = 0;
    int cnt =  0;
    int r;
    int num;
    cout<<"Enter the 4 digit number"<<endl;
    cin>>num;
    while(num){
        r = num%10;
        num = num/10;
        
        if(cnt<2){
            sum = sum +r;
        }else{
            anotherSum = anotherSum +r;
        }
        cnt++;
    }
    cout<<sum<<endl;
    cout<<anotherSum<<endl;
}
